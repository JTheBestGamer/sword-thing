local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("Sword")

local KillingCheats = PhantomForcesWindow:NewSection("Reach")

KillingCheats:CreateButton("Sword X", function()
local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid://413861777"
sound.Parent = game:GetService("SoundService")
sound:Play()
wait()
game.StarterGui:SetCore("SendNotification", {Title = "Notification", Text = "If you don't have a tool, equip one then click this", Duration = 5})

local localPlayer = game:GetService('Players').LocalPlayer

for i,v in pairs(localPlayer.Character:GetChildren()) do
    if v:IsA("Tool") then
        local toolName = v.Name:match("(.+) Reach [XYZ]")
        if toolName then
            v.Name = toolName
        end

        local a = Instance.new("SelectionBox", v.Handle)
        v.Name = v.Name .. " Reach X"
        v.Handle.Massless = true
        v.Handle.Transparency = 1
        a.Adornee = v.Handle
        v.Handle.Size = Vector3.new(1000, 2.5, 2.5)
        local selectionBox = Instance.new("SelectionBox", v.Handle)
        selectionBox.Adornee = v.Handle
        selectionBox.Color3 = Color3.new(0, 0.313725, 0.47451)
        selectionBox.Transparency = 1
    end
end
end)

KillingCheats:CreateButton("Sword Y", function()
local sound = Instance.new("Sound")
