local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("SwordStuff") -- Creates the window

local b = w:CreateFolder("brokenhitbox") -- Creates the folder(U will put here your buttons,etc)

b:Button("Sword X",function()
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local brokenhitboxX = Instance.new("TextButton")
ScreenGui.Parent = game.CoreGui ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(0, 200, 255)
Frame.BorderColor3 = Color3.fromRGB(25, 25, 255)
Frame.BorderSizePixel = 0.1
Frame.Position = UDim2.new(0.0809101239, 0, 0.203790441, 0)
Frame.Size = UDim2.new(0, 150, 0, 90)
Frame.Active = true
Frame.Draggable = true
brokenhitboxX.Name = "brokenhitboxX"
brokenhitboxX.Parent = Frame
brokenhitboxX.BackgroundColor3 = Color3.fromRGB(0, 255, 255)
brokenhitboxX.BorderSizePixel = 0
brokenhitboxX.Position = UDim2.new(0, 0, 0.139088048, 0)
brokenhitboxX.Size = UDim2.new(0, 143, 0, 38)
brokenhitboxX.Font = Enum.Font.GothamBlack
brokenhitboxX.Text = "brokenhitbox X"
brokenhitboxX.TextColor3 = Color3.fromRGB(255, 255, 255)
brokenhitboxX.TextSize = 14.000
brokenhitboxX.MouseButton1Down:connect(function()
local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid://413861777"
sound.Parent = game:GetService("SoundService") 	sound:Play()  	wait() 	game.StarterGui:SetCore("SendNotification", {Title = "Notification"; Text = "If you dont have a tool, equip one then click this"; Duration = 5; })
for i,v in pairs(game:GetService'Players'.LocalPlayer.Character:GetChildren()) do
if v:isA("Tool") then
local a = Instance.new("SelectionBox",v.Handle)
v.Name = "brokenhitbox thing X"
v.Handle.Massless = true
v.Handle.Transparency = 1
a.Adornee = v.Handle
v.Handle.Size = Vector3.new(1000, 2.5, 2.5)
local selectionBox = Instance.new("SelectionBox",v.Handle)
selectionBox.Adornee = v.Handle
selectionBox.Color3 = Color3.new(0, 0.313725, 0.47451)
selectionBox.Transparency = 1
end
end
end)
end)

